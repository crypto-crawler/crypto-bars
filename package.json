{
  "name": "crypto-bars",
  "version": "1.0.0",
  "description": "Crawl data from cryptocurrency exchanges and build bars in realtime.",
  "bin": "dist/cli.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "prepare": "npm run build && npm run lint && npm run prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/crypto-bars.git"
  },
  "keywords": [
    "bitcoin",
    "blockchain",
    "cryptocurrency"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/crypto-bars/issues"
  },
  "homepage": "https://github.com/soulmachine/crypto-bars",
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/lodash": "^4.14.154",
    "@types/node": "^14.0.6",
    "@types/redis": "^2.8.22",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/form-data": "^2.5.0",
    "chalk": "^4.0.0",
    "coin-bbo": "^0.5.7",
    "crypto-crawler": "^2.9.4",
    "crypto-markets": "^0.3.1",
    "fastpriorityqueue": "^0.6.3",
    "figlet": "^1.4.0",
    "lodash": "^4.17.15",
    "pm2": "^4.4.0",
    "redis": "^3.0.2",
    "rotating-file-stream": "^2.1.1",
    "technicalindicators": "^3.1.0",
    "winston": "^3.2.1",
    "yargs": "^15.3.1"
  }
}
